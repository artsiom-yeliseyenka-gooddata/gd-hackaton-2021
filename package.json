  "author": "GoodData Corporation",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "webpack serve --node-env=development --hot --https",
    "prepublishOnly": "yarn build",
    "build": "webpack --mode=production",
    "storybook": "start-storybook NODE_ENV=development -p 6006 -c .storybook",
    "build-storybook": "build-storybook -o dist-storybook -c .storybook",
    "install-with-retry": "./scripts/yarn-install-with-retry.sh",
    "prettier-check": "prettier --check '{src,styles,__mocks__}/**/*.{ts,tsx,scss,md,yaml,html}'",
    "prettier-write": "prettier --write '{src,styles,__mocks__}/**/*.{ts,tsx,scss,md,yaml,html}'",
    "validate": "eslint $npm_package_config_tslint && yarn prettier-check",
    "validate-ci": "eslint $npm_package_config_tslint -t checkstyle -o ci/results/eslint-results.xml && yarn prettier-check"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@gooddata/sdk-backend-bear": "^8.5.0",
    "@gooddata/sdk-backend-spi": "^8.5.0",
    "@gooddata/sdk-model": "^8.5.0",
    "@gooddata/sdk-ui": "^8.5.0",
    "@gooddata/sdk-ui-ext": "^8.5.0",
    "@mui/material": "^5.1.0",
    "@types/react-color": "^3.0.6",
    "@types/react-router-dom": "^5.1.7",
    "classnames": "^2.2.6",
    "formik": "^2.2.6",
    "html-react-parser": "^1.2.4",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.21",
    "process": "^0.11.10",
    "react": "^16.14.0",
    "react-color": "^2.19.3",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@gooddata/eslint-config": "^2.1.0",
    "@types/classnames": "^2.2.11",
    "@types/react": "^16.14.4",
    "@types/react-dom": "^16.9.11",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.8.3",
    "css-loader": "^5.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.1.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "regenerator-runtime": "^0.13.7",
    "sass-loader": "^10.1.1",
    "sass-resources-loader": "^2.1.1",
    "style-loader": "^2.0.0",
    "svg-inline-loader": "^0.8.2",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "util": "^0.12.3",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}": [
      "prettier --write",
      "git add"
    ],
    "{src,styles,__mocks__}./**/*.{ts,js,tsx,jsx,scss}": [
      "npx gdc-fix-copyright",
      "git add"
    ]
  }
